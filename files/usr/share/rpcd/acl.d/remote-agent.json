{
  "remote-agent": {
    "description": "ACL for remote-agent ubus object and required reads",
    "read": {
      "ubus": {
        "remote.agent": [
          "capabilities.list",
          "report.get",
          "session_pull_only"
        ],
        "system": [
          "*"
        ],
        "network": [
          "*"
        ],
        "iwinfo": [
          "*"
        ],
        "hostapd.*": [
          "get_clients"
        ]
      },
      "file": {
        "/etc/remote-agent/uuid": [
          "read"
        ],
        "/etc/openwrt_version": [
          "read"
        ],
        "/tmp/sysinfo/board_name": [
          "read"
        ]
      }
    },
    "write": {
      "ubus": {
        "remote.agent": [
          "session_pull_only"
        ]
      }
    }
  }
}